// For loop nested break


print "before";

for (var i = 0;;i = i + 1) {
    if (i >= 3) break;
    for (var j = 10;;j = j + 1) {
        if (j >= 13) break;
        for (var k = 100;;k = k + 1) {
            if (k >= 103) break;    
            print k;
        }
        print j;
    }
    print i;
}

print "after";

