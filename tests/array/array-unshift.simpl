// Array.unshift

var arr = [4, 5, 6];

print arr.unshift("c");                                                                      // expect 4
print arr.unshift("b");                                                                      // expect 5
print arr.unshift("a");                                                                      // expect 6
print arr;                                                                                   // expect [a, b, c, 4, 5, 6]
