// Math.min

System.log(Math.min(-1, 0));                                               // expect -1
System.log(Math.min(-1.5, 0));                                             // expect -1.5
System.log(Math.min(1, 3));                                                // expect 1
System.log(Math.min(0, 5));                                                // expect 0
System.log(Math.min(-10, 3));                                              // expect -10
System.log(Math.min(10, 60));                                              // expect 10

