// For each loop nested continue

System.log("before");

for (i of [0, 1, 2]) {
    if (i == 1) continue;
    for (j of [10, 11, 12]) {
        if (j == 11) continue;
        for (k of [100, 101, 102]) {
            if (k == 101) continue;
            System.log(k);
        }
        System.log(j);
    }
    System.log(i);
}

System.log("after");

