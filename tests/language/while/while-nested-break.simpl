print "before";

var i = 0;
while (true) {
    var j = 10;
    while (true) {
        var k = 100;
        while (true) {
            print k;
            k = k + 1;
            if (k >= 103) break;    
        }
        print j;
        j = j + 1;
        if (j >= 13) break;
    }
    print i;
    i = i + 1;
    if (i >= 3) break;
}

print "after";

