// While loop nested continue

print "before";

var i = 0;
while (i < 3) {
    if (i == 1) {
        i = i + 1;
        continue;
    }
    var j = 10;
    while (j < 13) {
        if (j == 11) {
            j = j + 1;
            continue;
        }
        var k = 100;
        while (k < 103) {
            if (k == 101) {
                k = k + 1;
                continue;
            }
            print k;
            k = k + 1;
        }
        print j;
        j = j + 1;
    }
    print i;
    i = i + 1;
}

print "after";

