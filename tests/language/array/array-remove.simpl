// Array.remove

System.log(["a", "b", "c", "d"].remove(2, 1));                   // expect [a, b, d]
System.log(["a", "b", "c", "d", "e", "f"].remove(2, 2));         // expect [a, b, e, f]
System.log(["a", "b", "c", "d"].remove(0, 1));                   // expect [b, c, d]
System.log(["a", "b", "c", "d"].remove(3, 1));                   // expect [a, b, c]
System.log(["a", "b", "c", "d"].remove(1, 3));                   // expect [a]
System.log(["a", "b", "c", "d"].remove(2, 0));                   // expect [a, b, c, d]
System.log(["a", "b", "c", "d"].remove(4, 1));                   // expect [a, b, c, d]
System.log([].remove(0, 1));                                     // expect []
System.log([].remove(1, 1));                                     // expect []
System.log(["a", "b", "c", "d"].remove(0, 4));                   // expect []
System.log(["a", "b", "c", "d"].remove(0, 10));                  // expect []
System.log(["a", "b", "c", "d"].remove(-1, 1));                  // expect [a, b, c]
System.log(["a", "b", "c", "d"].remove(-2, 1));                  // expect [a, b, d]
System.log(["a", "b", "c", "d"].remove(-3, 2));                  // expect [a, d]
System.log(["a", "b", "c", "d"].remove(-3, 4));                  // expect [a]
System.log(["a", "b", "c", "d"].remove(-4, 1));                  // expect [b, c, d]
System.log(["a", "b", "c", "d"].remove(-5, 1));                  // expect [a, b, c, d]
System.log(["a", "b", "c", "d"].remove(-1, 0));                  // expect [a, b, c, d]
System.log([].remove(-1, 1));                                    // expect []
System.log(["a", "b", "c", "d"].remove(-2, 0));                  // expect [a, b, c, d]
System.log(["a", "b", "c", "d"].remove(-10, 2));                 // expect [a, b, c, d]
