// Array.unshift

var arr = [4, 5, 6];

System.log(arr.unshift("c"));                                                                      // expect 4
System.log(arr.unshift("b"));                                                                      // expect 5
System.log(arr.unshift("a"));                                                                      // expect 6
System.log(arr);                                                                                   // expect [a, b, c, 4, 5, 6]
