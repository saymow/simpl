// Array.push

var arr = [1, 2, 3];

System.log(arr.push("d"));                                                                         // expect 4
System.log(arr.push("e"));                                                                         // expect 5
System.log(arr.push("f"));                                                                         // expect 6
System.log(arr);                                                                                   // expect [1, 2, 3, d, e, f]
