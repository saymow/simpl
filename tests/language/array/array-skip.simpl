// Array.skip

var arr = [1, 2, 3, 4, 5];

System.log(arr.skip(1));                                    // expect [2, 3, 4, 5]
System.log(arr.skip(0));                                    // expect [1, 2, 3, 4, 5]
System.log(arr.skip(2));                                    // expect [3, 4, 5]
System.log(arr.skip(-1));                                   // expect [1, 2, 3, 4, 5]
System.log(arr.skip(99));                                   // expect []