// Class method overloading - should not change superclass overloading 

class SuperTest {
    test() { print "arity 0"; }
    test(a) { print "arity 1"; }
    test(a, b) { print "arity 2"; }
    test(a, b, c) { print "arity 3"; }
    test(a, b, c, d) { print "arity 4"; }
    test(a, b, c, d, e) { print "arity 5"; }
    test(a, b, c, d, e, f) { print "arity 6"; }
    test(a, b, c, d, e, f, g) { print "arity 7"; }
    test(a, b, c, d, e, f, g, h) { print "arity 8"; }
}

class Test extends SuperTest {
    test(a, b, c, d, e, f, g, h) { print "definitely arity 8"; }
    test(a, b, c, d, e, f, g, h, i) { print "arity 9"; }
    test(a, b, c, d, e, f, g, h, i, j) { print "arity 10"; }
    test(a, b, c, d, e, f, g, h, i, j, k) { print "arity 11"; }
    test(a, b, c, d, e, f, g, h, i, j, k, l) { print "arity 12"; }
    test(a, b, c, d, e, f, g, h, i, j, k, l, m) { print "arity 13"; }
    test(a, b, c, d, e, f, g, h, i, j, k, l, m, n) { print "arity 14"; }
    test(a, b, c, d, e, f, g, h, i, j, k, l, m, n, o) { print "arity 15"; }
}

var superTestInstance = SuperTest();
var instance = Test();

instance.test();                                                                                    // expect arity 0
instance.test("a");                                                                                 // expect arity 1
instance.test("a", "b");                                                                            // expect arity 2
instance.test("a", "b", "c");                                                                       // expect arity 3
instance.test("a", "b", "c", "d");                                                                  // expect arity 4
instance.test("a", "b", "c", "d", "e");                                                             // expect arity 5
instance.test("a", "b", "c", "d", "e", "f");                                                        // expect arity 6
instance.test("a", "b", "c", "d", "e", "f", "g");                                                   // expect arity 7
instance.test("a", "b", "c", "d", "e", "f", "g", "h");                                              // expect definitely arity 8
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i");                                         // expect arity 9
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j");                                    // expect arity 10
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k");                               // expect arity 11
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l");                          // expect arity 12
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m");                     // expect arity 13
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n");                // expect arity 14
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n", "o");           // expect arity 15


superTestInstance.test("a", "b", "c", "d", "e", "f", "g", "h");                                     // expect arity 8
superTestInstance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n", "o");  // error SOFTWARE_ERR