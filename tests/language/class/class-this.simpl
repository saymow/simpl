// Class this keyword

class Person {
    sayName() {
        System.log("my name is " + this.name + " " + this.lastname);
    }
}

var person = Person(); 

person.name = "John";
person.lastname = "Doe";

person.sayName();                                                     // expect my name is John Doe
person["sayName"]();                                                  // expect my name is John Doe