// Class method overloading

class Test {
    test() { System.log("arity 0"); }
    test(a) { System.log("arity 1"); }
    test(a, b) { System.log("arity 2"); }
    test(a, b, c) { System.log("arity 3"); }
    test(a, b, c, d) { System.log("arity 4"); }
    test(a, b, c, d, e) { System.log("arity 5"); }
    test(a, b, c, d, e, f) { System.log("arity 6"); }
    test(a, b, c, d, e, f, g) { System.log("arity 7"); }
    test(a, b, c, d, e, f, g, h) { System.log("arity 8"); }
    test(a, b, c, d, e, f, g, h, i) { System.log("arity 9"); }
    test(a, b, c, d, e, f, g, h, i, j) { System.log("arity 10"); }
    test(a, b, c, d, e, f, g, h, i, j, k) { System.log("arity 11"); }
    test(a, b, c, d, e, f, g, h, i, j, k, l) { System.log("arity 12"); }
    test(a, b, c, d, e, f, g, h, i, j, k, l, m) { System.log("arity 13"); }
    test(a, b, c, d, e, f, g, h, i, j, k, l, m, n) { System.log("arity 14"); }
    test(a, b, c, d, e, f, g, h, i, j, k, l, m, n, o) { System.log("arity 15"); }
}

var instance = Test();

instance.test();                                                                                    // expect arity 0
instance.test("a");                                                                                 // expect arity 1
instance.test("a", "b");                                                                            // expect arity 2
instance.test("a", "b", "c");                                                                       // expect arity 3
instance.test("a", "b", "c", "d");                                                                  // expect arity 4
instance.test("a", "b", "c", "d", "e");                                                             // expect arity 5
instance.test("a", "b", "c", "d", "e", "f");                                                        // expect arity 6
instance.test("a", "b", "c", "d", "e", "f", "g");                                                   // expect arity 7
instance.test("a", "b", "c", "d", "e", "f", "g", "h");                                              // expect arity 8
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i");                                         // expect arity 9
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j");                                    // expect arity 10
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k");                               // expect arity 11
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l");                          // expect arity 12
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m");                     // expect arity 13
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n");                // expect arity 14
instance.test("a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n", "o");           // expect arity 15