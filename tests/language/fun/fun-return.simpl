// Function with return

fun mult(a, b) {
    return a * b;
}

fun division(a, b) {
    return a / b;
    System.log("unreachable");
}

System.log(mult(1, 2));                                                          // expect 2
System.log(mult(3, 5));                                                          // expect 15
System.log(mult(5, -2));                                                         // expect -10

System.log(division(1, 2));                                                      // expect 0.5
System.log(division(3, 5));                                                      // expect 0.6
System.log(division(5, -2));                                                     // expect -2.5