// Function currying

fun sumBy(a) {
    fun sum(b) {
        return a + b;
    }

    return sum;                                                        
}

fun multBy(a) {
    return fun (b) {
        return a * b;
    };                                                        
}

fun concatenateBy(str) {
    return fun (str2) {
        return str + str2;
    }; 
}

var sumBy5 = sumBy(5);
var multBy5 = multBy(5);
var concatenateByHello = concatenateBy("Hello"); 

print sumBy5(3);                                                                 // expect 8
print sumBy5(9);                                                                 // expect 14
print sumBy5(-5);                                                                // expect 0

print multBy5(3);                                                                // expect 15
print multBy5(9);                                                                // expect 45
print multBy5(-5);                                                               // expect -25

print concatenateByHello(" World");                                              // expect Hello World
print concatenateByHello(" Earth");                                              // expect Hello Earth
print concatenateByHello(" Lng");                                                // expect Hello Lng
