// Intensively test table lookup access  

class Zoo {
    Zoo() {
        this.aardvark = 1;
        this.baboon = 1;
        this.cat = 1;
        this.donkey = 1;
        this.elephant = 1;
        this.fox = 1;
    }

    ant() { return this.aardvark; }
    banana() { return this.baboon; }
    tuna() { return this.cat; }
    hay() { return this.donkey; }
    grass() { return this.elephant; }
    mouse() { return this.fox; }
}
var zoo = Zoo();

var sum = 0;
var start = clock();

while (sum < 50000000) {
 sum = sum + zoo.ant()
    + zoo.banana()
    + zoo.tuna()
    + zoo.hay()
    + zoo.grass()
    + zoo.mouse();
}

print sum;
print clock() - start;


