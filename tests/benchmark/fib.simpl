// Fibonacci implementation to test calls and arithmetic expressions.

var N = 40;

fun fib(n) {
    if (n <= 1) return n;
    return fib(n - 2) + fib(n - 1);
}

var start = System.clock();

fib(N);

System.log(System.clock() - start);

